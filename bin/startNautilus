ID=`/usr/bin/whoami`

echo 'usage: ssh -L 8443:127.0.0.1:8443 -N -f -l joshjua dev.nautilustechnologies.io'
echo '     : browse to http://localhost:8443'

docker run -d \
  -p 127.0.0.1:8443:8443 \
  -v /cache/shared/${ID}:/home/coder/project \
  --user $(id -u):$(id -g) \
  alex2006hw/vsceditor:1.0.0 --allow-http --no-auth

